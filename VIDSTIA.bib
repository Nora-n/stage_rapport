	pages = {11},
	file = {Full Text PDF:/home/nora/Zotero/storage/YIZUH3LR/Guy et al. - 2007 - SALT2 using distant supernovae to improve the use.pdf:application/pdf;Snapshot:/home/nora/Zotero/storage/2ACQVRZD/abstract.html:text/html}
