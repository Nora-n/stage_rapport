@article{madau_cosmic_2014,
	title = {Cosmic {Star} {Formation} {History}},
	volume = {52},
	issn = {0066-4146, 1545-4282},
	url = {http://arxiv.org/abs/1403.0007},
	doi = {10.1146/annurev-astro-081811-125615},
	abstract = {Over the past two decades, an avalanche of data from multiwavelength imaging and spectroscopic surveys has revolutionized our view of galaxy formation and evolution. Here we review the range of complementary techniques and theoretical tools that allow astronomers to map the cosmic history of star formation, heavy element production, and reionization of the Universe from the cosmic "dark ages" to the present epoch. A consistent picture is emerging, whereby the star-formation rate density peaked approximately 3.5 Gyr after the Big Bang, at z{\textasciitilde}1.9, and declined exponentially at later times, with an e-folding timescale of 3.9 Gyr. Half of the stellar mass observed today was formed before a redshift z = 1.3. About 25\% formed before the peak of the cosmic star-formation rate density, and another 25\% formed after z = 0.7. Less than {\textasciitilde}1\% of today's stars formed during the epoch of reionization. Under the assumption of a universal initial mass function, the global stellar mass density inferred at any epoch matches reasonably well the time integral of all the preceding star-formation activity. The comoving rates of star formation and central black hole accretion follow a similar rise and fall, offering evidence for co-evolution of black holes and their host galaxies. The rise of the mean metallicity of the Universe to about 0.001 solar by z = 6, one Gyr after the Big Bang, appears to have been accompanied by the production of fewer than ten hydrogen Lyman-continuum photons per baryon, a rather tight budget for cosmological reionization.},
	number = {1},
	urldate = {2019-08-14},
	journal = {Annual Review of Astronomy and Astrophysics},
	author = {Madau, Piero and Dickinson, Mark},
	month = aug,
	year = {2014},
	note = {arXiv: 1403.0007},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics},
	pages = {415--486},
	file = {arXiv\:1403.0007 PDF:/home/nora/Zotero/storage/5MRQCNQ7/Madau and Dickinson - 2014 - Cosmic Star Formation History.pdf:application/pdf}
}

@article{mannucci_supernova_2005,
	title = {The supernova rate per unit mass},
	volume = {433},
	url = {https://ui.adsabs.harvard.edu/abs/2005A%26A...433..807M/abstract},
	doi = {10.1051/0004-6361:20041411},
	abstract = {We compute the rate of supernovae (SNe) of different types along the Hubble sequence normalized to the near-infrared luminosity and to the stellar mass of the parent galaxies. This is made possible by the new complete catalog of near-infrared galaxy magnitudes obtained by 2MASS. We find that the rates of all SN types, including Ia, Ib/c and II, show a sharp dependence on both the morphology and the (B-K) colors of the parent galaxies and, therefore, on the star formation activity. In particular we find, with a high statistical significance, that the type Ia rate in late type galaxies is a factor {\textasciitilde}20 higher than in E/S0. Similarly, the type Ia rate in the galaxies bluer than B-K=2.6 is about a factor of 30 larger than in galaxies with B-K\&gt;4.1. These findings can be explained by assuming that a significant fraction of Ia events in late spirals/irregulars originates in a relatively young stellar component.},
	language = {en},
	number = {3},
	urldate = {2019-08-14},
	journal = {Astronomy and Astrophysics},
	author = {Mannucci, F. and Della Valle, M. et al.},
	month = apr,
	year = {2005},
	pages = {807},
	file = {Full Text PDF:/home/nora/Zotero/storage/ANEEXIMV/Mannucci et al. - 2005 - The supernova rate per unit mass.pdf:application/pdf;Snapshot:/home/nora/Zotero/storage/WJULKVB9/abstract.html:text/html}
}

@article{scannapieco_type_2005,
	title = {The {Type} {Ia} {Supernova} {Rate}},
	volume = {629},
	url = {https://ui.adsabs.harvard.edu/abs/2005ApJ...629L..85S/abstract},
	doi = {10.1086/452632},
	abstract = {We explore the idea that the Type Ia supernova (SN Ia) rate consists of two components: a prompt piece that is proportional to the star formation rate (SFR), and an extended piece that is proportional to the total stellar mass. We fit the parameters of this model to the local observations by Mannucci and collaborators and then study its impact on three important problems. On cosmic scales, the model reproduces the observed SN Ia rate density below z=1 and predicts that it will track the measured SFR density at higher redshift, reaching a value of (1-3.5)×10{\textless}SUP{\textgreater}-4{\textless}/SUP{\textgreater} yr{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater} Mpc{\textless}SUP{\textgreater}-3{\textless}/SUP{\textgreater} at z=2. In galaxy clusters, a large prompt contribution helps explain the Fe content of the intracluster medium. Within the Galaxy, the model reproduces the observed stellar [O/Fe] abundance ratios if we allow a short ({\textasciitilde}0.7 Gyr) delay in the prompt component. Ongoing medium-z SN surveys will yield more accurate parameters for our model.},
	language = {en},
	number = {2},
	urldate = {2019-08-14},
	journal = {The Astrophysical Journal},
	author = {Scannapieco, Evan and Bildsten, Lars},
	month = aug,
	year = {2005},
	pages = {L85},
	file = {Full Text PDF:/home/nora/Zotero/storage/CEFCKDA3/Scannapieco and Bildsten - 2005 - The Type Ia Supernova Rate.pdf:application/pdf;Snapshot:/home/nora/Zotero/storage/X8VSRMKL/abstract.html:text/html}
}

@article{sullivan_rates_2006,
	title = {Rates and {Properties} of {Type} {Ia} {Supernovae} as a {Function} of {Mass} and {Star} {Formation} in {Their} {Host} {Galaxies}},
	volume = {648},
	url = {https://ui.adsabs.harvard.edu/abs/2006ApJ...648..868S/abstract},
	doi = {10.1086/506137},
	abstract = {We show that Type Ia supernovae (SNe Ia) are formed within both very young and old stellar populations, with observed rates that depend on the stellar mass and mean star formation rates (SFRs) of their host galaxies. Models in which the SN Ia rate depends solely on host galaxy stellar mass are ruled out with \&gt;99\% confidence. Our analysis is based on 100 spectroscopically confirmed SNe Ia, plus 24 photometrically classified events, all from the Supernova Legacy Survey (SNLS) and distributed over 0.2\&lt;z\&lt;0.75. We estimate stellar masses and SFRs for the SN Ia host galaxies by fitting their broadband spectral energy distributions with the galaxy spectral synthesis code PÉGASE.2. We show that the SN Ia rate per unit mass is proportional to the specific SFR of the parent galaxies-more vigorously star-forming galaxies host more SNe Ia per unit stellar mass, broadly equivalent to the trend of increasing SN Ia rate in later type galaxies seen in the local universe. Following earlier suggestions for a simple ``two-component'' model approximating the SN Ia rate, we find bivariate linear dependencies of the SN Ia rate on both the stellar masses and the mean SFRs of the host systems. We find that the SN Ia rate can be well represented as the sum of 5.3+/-1.1×10{\textless}SUP{\textgreater}-14{\textless}/SUP{\textgreater} SNe yr{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater} M{\textless}SUB{\textgreater}solar{\textless}/SUB{\textgreater}{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater} and 3.9+/-0.7×10{\textless}SUP{\textgreater}-4{\textless}/SUP{\textgreater} SNe yr{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater} (M{\textless}SUB{\textgreater}solar{\textless}/SUB{\textgreater} yr{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater}){\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater} of star formation. We also demonstrate a dependence of distant SN Ia light-curve shapes on star formation in the host galaxy, similar to trends observed locally. Passive galaxies, with no star formation, preferentially host faster declining/dimmer SNe Ia, while brighter events are found in systems with ongoing star formation.},
	language = {en},
	number = {2},
	urldate = {2019-08-14},
	journal = {The Astrophysical Journal},
	author = {Sullivan, M. and Le Borgne, D. et al.},
	month = sep,
	year = {2006},
	pages = {868},
	file = {Full Text PDF:/home/nora/Zotero/storage/CCE9VGFC/Sullivan et al. - 2006 - Rates and Properties of Type Ia Supernovae as a Fu.pdf:application/pdf;Snapshot:/home/nora/Zotero/storage/34HVFQUK/abstract.html:text/html}
}

@article{tasca_evolving_2015,
	title = {The evolving star formation rate: {M}{\textless}{SUB}{\textgreater}⋆{\textless}/{SUB}{\textgreater} relation and {sSFR} since z ≃ 5 from the {VUDS} spectroscopic survey},
	volume = {581},
	shorttitle = {The evolving star formation rate},
	url = {https://ui.adsabs.harvard.edu/abs/2015A%26A...581A..54T/abstract},
	doi = {10.1051/0004-6361/201425379},
	abstract = {We study the evolution of the star formation rate (SFR) - stellar mass (M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater}) relation and specific star formation rate (sSFR) of star-forming galaxies (SFGs) since a redshift z ≃ 5.5 using 2435 (4531) galaxies with highly reliable spectroscopic redshifts in the VIMOS Ultra-Deep Survey (VUDS). It is the first time that these relations can be followed over such a large redshift range from a single homogeneously selected sample of galaxies with spectroscopic redshifts. The log (SFR) - log (M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater}) relation for SFGs remains roughly linear all the way up to z = 5, but the SFR steadily increases at fixed mass with increasing redshift. We find that for stellar masses M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater} ≥ 3.2 × 10{\textless}SUP{\textgreater}9{\textless}/SUP{\textgreater}M{\textless}SUB{\textgreater}☉{\textless}/SUB{\textgreater} the SFR increases by a factor of {\textasciitilde}13 between z = 0.4 and z = 2.3. Weextend this relation up to z = 5, finding an additional increase in SFR by a factor of 1.7 from z = 2.3 to z = 4.8 for masses M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater} ≥ 10{\textless}SUP{\textgreater}10{\textless}/SUP{\textgreater}M{\textless}SUB{\textgreater}☉{\textless}/SUB{\textgreater}. We observe a turn-off in the SFR-M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater} relation at the highest mass end up to a redshift z {\textasciitilde} 3.5. We interpret this turn-off as the signature of a strong on-going quenching mechanism and rapid mass growth. The sSFR increases strongly up to z {\textasciitilde} 2, but it grows much less rapidly in 2 \&lt;z\&lt; 5. We find that the shape of the sSFR evolution is not well reproduced by cold gas accretion-driven models or the latest hydrodynamical models. Below z {\textasciitilde} 2 these models have a flatter evolution (1 + z){\textless}SUP{\textgreater}Φ{\textless}/SUP{\textgreater} with Φ = 2 - 2.25 compared to the data which evolves more rapidly with Φ = 2.8 ± 0.2. Above z {\textasciitilde} 2, the reverse is happening with the data evolving more slowly with Φ = 1.2 ± 0.1. The observed sSFR evolution over a large redshift range 0 \&lt;z\&lt; 5 and our finding of a non-linear main sequence at high mass both indicate that the evolution of SFR and M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater} is not solely driven by gas accretion. The results presented in this paper emphasize the need to invoke a more complex mix of physical processes including major and minor merging to further understand the co-evolution of the SFR and stellar mass growth. Based on data obtained with the European Southern Observatory Very Large Telescope, Paranal, Chile, under Large Program 185.A-0791.},
	language = {en},
	urldate = {2019-08-14},
	journal = {Astronomy and Astrophysics},
	author = {Tasca, L. a. M. and Le Fèvre, O. et al.},
	month = sep,
	year = {2015},
	pages = {A54},
	file = {Full Text PDF:/home/nora/Zotero/storage/XD6D2KB8/Tasca et al. - 2015 - The evolving star formation rate MSUB⋆SUB re.pdf:application/pdf;Snapshot:/home/nora/Zotero/storage/8P5LPWJJ/abstract.html:text/html}
}
